{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "skipLibCheck": true,

    /* Node.js specific */
    "outDir": "./build",
    "typeRoots": ["node_modules/@types"],
    "moduleResolution": "node"
  },
  "include": ["public/src","public/src/js/index/main.ts", "build/main.js"]
}


